<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="390dp"
    android:layout_height="108dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_marginHorizontal="10dp"
    android:layout_marginBottom="7dp"
    >

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <androidx.cardview.widget.CardView
            android:id="@+id/bordergambar"
            android:layout_width="94dp"
            android:layout_height="94dp"
            app:cardCornerRadius="5dp"
            android:layout_marginVertical="6.6dp"
            android:layout_marginStart="9.92dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            >

            <com.google.android.material.imageview.ShapeableImageView
                android:id="@+id/gambarjasa"
                android:layout_width="94dp"
                android:layout_height="94dp"
                app:srcCompat="@drawable/a"
                android:scaleType="centerCrop"
                />

        </androidx.cardview.widget.CardView>


        <TextView
            android:id="@+id/namajasa"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="BUNDLING FULL PACKAGE PRE+WEDDING"
            android:textSize="13sp"
            android:layout_marginStart="11dp"
            android:layout_marginTop="9dp"
            app:layout_constraintTop_toTopOf="@id/bordergambar"
            app:layout_constraintStart_toEndOf="@id/bordergambar"
            android:textColor="@color/black"
            android:textStyle="bold"
            />

        <TextView
            android:id="@+id/namavendor"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="by Java Bride - Wedding Planner"
            android:textSize="10sp"
            android:layout_marginTop="2.39dp"
            app:layout_constraintTop_toBottomOf="@id/namajasa"
            app:layout_constraintStart_toStartOf="@id/namajasa"
            android:textColor="@color/black"
            />

        <TextView
            android:id="@+id/hargaproduct"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Rp25.000.0000"
            android:textSize="17sp"
            android:layout_marginTop="2.39dp"
            app:layout_constraintTop_toBottomOf="@id/namavendor"
            app:layout_constraintStart_toStartOf="@id/namajasa"
            android:textColor="@color/black"
            android:textStyle="bold"
            />


